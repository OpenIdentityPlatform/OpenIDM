{
    "server-auth-config" : {
        "iwa-only" : {
            "auth-module" : {
                "class-name" : "org.forgerock.jaspi.modules.IWAModule"
            }
        },
        "ad-passthrough-only" : {
            "auth-module" : {
                "class-name" : "org.forgerock.jaspi.modules.adpassthrough.ADPassthroughModule",
                "passThroughAuth" : "system/AD/account",
                "queryId" : "credential-query",
                "queryOnResource" : "managed/user",
                "propertyMapping" : {
                    "userId" : "_id",
                    "userCredential" : "password",
                    "userRoles" : "memberOf"
                },
                "defaultUserRoles" : [
                    "openidm-reg"
                ],
                "allowedGroups" : [ ]
            }
        }
    }
}