{
  "serverAuthContext" : {
    "authModules" : [
      {
        "name" : "OAUTH",
        "properties" : {
          "queryOnResource" : "managed/user",
          "propertyMapping" : {
            "userRoles" : "authzRoles",
            "authenticationId" : "idpData/facebook/subject"
          },
          "defaultUserRoles" : [
            "openidm-authorized"
          ],
          "resolvers" : [
            {
              "name" : "facebook",
              "type" : "OAUTH",
              "icon" : "facebook",
              "scope" : [
                "email",
                "user_birthday"
              ],
              "authenticationId" : "id",
              "propertyMap" : [
                {
                  "source" : "name",
                  "target" : "displayName"
                },
                {
                  "source" : "first_name",
                  "target" : "name/givenName"
                },
                {
                  "source" : "last_name",
                  "target" : "name/familyName"
                },
                {
                  "source" : "email",
                  "target" : "email"
                },
                {
                  "source" : "email",
                  "target" : "username"
                },
                {
                  "source" : "locale",
                  "target" : "locale"
                }
              ],
              "enabled" : true,
              "client_id" : "",
              "client_secret" : "",
              "authorization_endpoint" : "https://www.facebook.com/dialog/oauth",
              "token_endpoint" : "https://graph.facebook.com/v2.7/oauth/access_token",
              "userinfo_endpoint" : "https://graph.facebook.com/me?fields=id,name,picture,email,first_name,last_name,locale"
            }
          ],
          "authTokenHeader" : "authToken",
          "authResolverHeader" : "provider"
        },
        "enabled" : true
      },
      {
        "name" : "OPENID_CONNECT",
        "properties" : {
          "queryOnResource" : "managed/user",
          "propertyMapping" : {
            "authenticationId" : "idpData/google/subject",
            "userRoles" : "authzRoles"
          },
          "defaultUserRoles" : [
            "openidm-authorized"
          ],
          "resolvers" : [
            {
              "name" : "google",
              "type" : "OPENID_CONNECT",
              "icon" : "google",
              "scope" : [
                "openid",
                "profile",
                "email"
              ],
              "authenticationId" : "sub",
              "enabled" : true,
              "client_id" : "",
              "client_secret" : "",
              "authorization_endpoint" : "https://accounts.google.com/o/oauth2/v2/auth",
              "token_endpoint" : "https://www.googleapis.com/oauth2/v4/token",
              "userinfo_endpoint" : "https://www.googleapis.com/oauth2/v3/userinfo",
              "well-known" : "https://accounts.google.com/.well-known/openid-configuration"
            }
          ],
          "openIdConnectHeader" : "authToken"
        },
        "enabled" : true
      }
    ]
  }
}