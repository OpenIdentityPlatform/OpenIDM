{
  "authModules": [
    {
      "name": "OPENID_CONNECT",
      "properties": {
        "openIdConnectHeader": "authToken",
        "queryOnResource": "managed/user",
        "propertyMapping": {
          "authenticationId": "sub",
          "userRoles": "authzRoles"
        },
        "defaultUserRoles": [
          "openidm-authorized"
        ]
      },
      "enabled": true
    },
    {
      "name": "OAUTH",
      "properties": {
        "authTokenHeader": "authToken",
        "authProviderHeader" : "provider",
        "queryOnResource": "managed/user",
        "propertyMapping": {
          "authenticationId": "id",
          "userRoles": "authzRoles"
        },
        "defaultUserRoles": [
          "openidm-authorized"
        ]
      },
      "enabled": true
    }
  ]
}
