{
    "auditServiceConfig" : {
        "handlerForQueries" : "csv"
    },
    "eventHandlers" : [
        {
            "name" : "csv",
            "class" : "org.forgerock.audit.events.handlers.impl.CSVAuditEventHandler",
            "config" : {
                "logDirectory" : "audit"
            },
            "events" : [ "access", "activity", "recon", "sync", "authentication" ]
        }
    ],
    "extendedEventTypes" : {
        "activity": {
            "schema": {
                "$schema": "http://json-schema.org/draft-04/schema#",
                "id": "/",
                "type": "object",
                "properties": {
                    "objectId": {
                        "type": "string"
                    },
                    "status": {
                        "type": "string"
                    },
                    "message": {
                        "type": "string"
                    },
                    "passwordChanged": {
                        "type": "boolean"
                    }
                }
            }
        }
    },
    "customEventTypes" : {
    }
}
