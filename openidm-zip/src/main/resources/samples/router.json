{
   "filters" :
      [
         {
            "pattern" : ".*",
            "methods" :
               [
                  "create",
                  "update",
                  "read",
                  "delete",
                  "query",
                  "action"
               ],
            "condition" :
               {
                  "type" : "text/javascript",
                  "source" : "id==id"
               },
            "onRequest" :
               {
                  "type" : "text/javascript",
                  "source" : "java.lang.System.out.println('Before: ' + method + '\\nId: ' + id);"
               },
            "onResponse" :
               {
                  "type" : "text/javascript",
                  "source" : "java.lang.System.out.println('After: ' + method + '\\nId: ' + id);"
               }
         },
         {
            "pattern" : "^managed/user/.*",
            "methods" :
               [
                  "create",
                  "update",
                  "read",
                  "delete",
                  "query",
                  "action"
               ],
            "onResponse" :
               {
                  "type" : "text/javascript",
                  "source" : "java.lang.System.out.println('After: ' + method + '\\nId: ' + id + '\\nobject: [object]\\nparams: [query,anction]\\nresult: [result]');"
               }
         }
      ]
}