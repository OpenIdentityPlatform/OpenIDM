{
   "filters" :
      [
         {
            "pattern" : ".*",
            "methods" :
               [
                  "create",
                  "update",
                  "read",
                  "delete",
                  "query",
                  "action"
               ],
            "condition" :
               {
                  "type" : "groovy",
                  "source" : "request.id==request.id"
               },
            "onRequest" :
               {
                  "type" : "groovy",
                  "source" : "println 'Before: ' + request.requestType + '\\nId: ' + request.resourceName"
               },
            "onResponse" :
               {
                  "type" : "groovy",
                  "source" : "println 'After: ' + request.requestType + '\\nId: ' + request.resourceName"
               }
         },
         {
            "pattern" : "^managed/user/.*",
            "methods" :
               [
                  "create",
                  "update",
                  "read",
                  "delete",
                  "query",
                  "action"
               ]
         }
      ]
}